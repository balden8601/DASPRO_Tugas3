        exportRoot.tl1.from(date, 1.4, {  x: "+=250",y: "+=250",	ease:Power4.easeOut}, "-=1.4");

        exportRoot.tl1.to(exportRoot.headline1, 0.1, {alpha: 0, ease:Power2.easeInOut, stagger:0.1}, "+=0.4")
